<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
       android:shape="oval">

    <padding
        android:bottom="10dp"
        android:left="10dp"
        android:right="10dp"
        android:top="10dp"/>

    <size
        android:width="@dimen/shape_oval_size"
        android:height="@dimen/shape_oval_size"/>

    <stroke
        android:width="5dp"
        android:color="@android:color/darker_gray"
        android:dashGap="7dp"
        android:dashWidth="10dp"/>
</shape>