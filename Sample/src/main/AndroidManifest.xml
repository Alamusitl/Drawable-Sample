<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.owl.drawablesample">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".ShapeActivity"/>
        <activity android:name=".BitmapActivity"/>
        <activity android:name=".NinePatchActivity"/>
        <activity android:name=".LayerActivity"/>
        <activity android:name=".StateActivity"/>
        <activity android:name=".LevelActivity"/>
        <activity android:name=".TransitionActivity"/>
        <activity android:name=".InsetActivity"/>
        <activity android:name=".ClipActivity"/>
        <activity android:name=".ScaleActivity"/>
    </application>

</manifest>